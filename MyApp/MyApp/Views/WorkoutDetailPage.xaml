<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:prism="http://prismlibrary.com"
             prism:ViewModelLocator.AutowireViewModel="True"
             x:Class="MyApp.Views.WorkoutDetailPage">

    <StackLayout HorizontalOptions="Center">
        <Image x:Name="MyImageCall" HeightRequest="300" WidthRequest="300"/>
        <Label x:Name="MyItemNameShow" HorizontalTextAlignment="Center"/>
        <Label x:Name="MyInstructionsItemShow" Margin="20,20" FontSize="Small"/>
    </StackLayout>

</ContentPage>